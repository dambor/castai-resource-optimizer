{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "frontend",
    "namespace": "default"
  },
  "spec": {
    "template": {
      "spec": {
        "containers": [
          {
            "name": "server",
            "resources": {
              "requests": {
                "cpu": "0.49",
                "memory": "40Mi"
              },
              "limits": {
                "memory": "128Mi"
              }
            }
          }
        ]
      }
    }
  }
}